# GO HAM PRO - Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_ENV=development

# Stripe Payment
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# Firebase (Optional)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef

# Email Service
REACT_APP_EMAIL_SERVICE_URL=https://api.sendgrid.com/v3
REACT_APP_SUPPORT_EMAIL=support@example.com

# SMS Service
REACT_APP_SMS_ENABLED=false

# Feature Flags
REACT_APP_ENABLE_BOOKING=true
REACT_APP_ENABLE_PAYMENTS=true
REACT_APP_ENABLE_CHAT=true
